{
  "name": "dt",
  "class": "sklearn.tree.DecisionTreeClassifier",
  "hyperparameters": {
    "criterion": {
      "type": "string",
      "values": [
        "entropy",
        "gini"
      ],
      "default": "gini"
    },
    "max_depth": {
      "type": "float",
      "range": [
        0.0,
        2.0
      ],
      "default": 0.5
    },
    "min_samples_split": {
      "type": "int",
      "range": [
        2,
        20
      ],
      "default": 2
    },
    "min_samples_leaf": {
      "type": "int",
      "range": [
        1,
        20
      ],
      "default": 1
    },
    "max_features": {
      "type": "float",
      "range": [
        0.0,
        1.0
      ],
      "default": 1
    }
  },
  "root_hyperparameters": [
    "criterion",
    "max_depth",
    "min_samples_split",
    "min_samples_leaf",
    "max_features"
  ],
  "conditional_hyperparameters": {}
}
