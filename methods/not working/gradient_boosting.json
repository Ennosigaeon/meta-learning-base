{
  "name": "gb",
  "class": "sklearn.ensemble.HistGradientBoostingClassifier",
  "hyperparameters": {
    "learning_rate": {
      "type": "float",
      "range": [
        0.01,
        1
      ],
      "default": 0.1
    },
    "max_iter": {
      "type": "int",
      "range": [
        32,
        512
      ],
      "default": 100
    },
    "min_samples_leaf": {
      "type": "int",
      "range": [
        1,
        200
      ],
      "default": 20
    },
    "max_leaf_nodes": {
      "type": "int",
      "range": [
        3,
        2047
      ],
      "default": 31
    },
    "l2_regularization": {
      "type": "float",
      "range": [
        1E-10,
        1
      ],
      "default": 1E-10
    },
    "n_iter_no_change": {
      "type": "int",
      "range": [
        1,
        20
      ],
      "default": 10
    },
    "validation_fraction": {
      "type": "float",
      "range": [
        0.01,
        0.4
      ],
      "default": 0.1
    }
  },
  "root_hyperparameters": [
    "learning_rate",
    "max_iter",
    "min_samples_leaf",
    "max_leaf_nodes",
    "l2_regularization",
    "n_iter_no_change",
    "validation_fraction"
  ],
  "conditional_hyperparameters": {}
}
