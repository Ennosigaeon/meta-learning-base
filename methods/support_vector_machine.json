{
  "class": "sklearn.svm.SVC",
  "hyperparameters": {
    "C": {
      "type": "float_exp",
      "range": [
        1e-5,
        1e5
      ],
      "default": 1.0
    },
    "gamma": {
      "type": "float_exp",
      "range": [
        1e-5,
        1e5
      ],
      "default": 0.1
    },
    "kernel": {
      "type": "string",
      "values": [
        "rbf",
        "poly",
        "linear",
        "sigmoid"
      ],
      "default": "rbf"
    },
    "degree": {
      "type": "int",
      "range": [
        2,
        5
      ],
      "default": 3
    },
    "coef0": {
      "type": "int",
      "range": [
        -1e3,
        1e3
      ],
      "default": 0.0
    },
    "probability": {
      "type": "bool",
      "default": false
    },
    "shrinking": {
      "type": "bool",
      "default": true
    }
  },
  "root_hyperparameters": [
    "C",
    "kernel",
    "probability",
    "shrinking"
  ],
  "conditional_hyperparameters": {
    "kernel": {
      "gamma": [
        "rbf",
        "sigmoid",
        "poly"
      ],
      "coef0": [
        "sigmoid",
        "poly"
      ],
      "degree": [
        "poly"
      ]
    }
  }
}
